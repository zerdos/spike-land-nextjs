{
  "name": "@spike-npm-land/spike-land-backend",
  "description": "Main backend for spike.land, providing real-time collaboration, API services, AI integrations, and static asset hosting via Cloudflare Workers.",
  "version": "0.9.56",
  "type": "module",
  "module": "dist/index.js",
  "private": true,
  "peerDependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "scripts": {
    "dev": "npx wrangler dev",
    "dev:remote": "npx wrangler dev --remote",
    "build:worker": "echo 'No build required - using direct deployment'",
    "w:deploy:dev": "npx wrangler deploy --env testing --minify",
    "w:deploy:prod": "npx wrangler deploy --minify",
    "clean": "rimraf dist",
    "lint": "eslint .",
    "types:check": "tsc --noEmit",
    "test:unit": "vitest run",
    "test:api": "yarn types:check && yarn lint && yarn test:unit",
    "test:watch": "vitest",
    "__STATIC_CONTENT_MANIFEST": "./src/staticContent.mjs"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20260212.0",
    "@eslint/js": "10.0.1",
    "@spike-npm-land/transpile": "0.9.56",
    "@types/mime-types": "3.0.1",
    "@typescript-eslint/parser": "8.55.0",
    "@vitest/runner": "4.0.18",
    "@vitest/snapshot": "4.0.18",
    "esbuild": "0.27.3",
    "eslint": "10.0.0",
    "globals": "17.3.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "rimraf": "6.1.2",
    "typescript": "5.9.3",
    "vitest": "4.0.18"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "3.0.42",
    "@ai-sdk/google": "^3.0.28",
    "@google/genai": "^1.41.0",
    "@spike-npm-land/code": "0.9.57",
    "@types/node": "25.2.3",
    "ai": "6.0.83",
    "esbuild-wasm": "0.27.3",
    "hono": "4.11.9",
    "immutable": "5.1.4",
    "mime-types": "3.0.2",
    "openai": "6.21.0",
    "replicate": "1.4.0",
    "zod": "4.3.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
