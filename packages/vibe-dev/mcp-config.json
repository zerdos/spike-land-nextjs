{
  "mcpServers": {
    "spike-land": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@spike-npm-land/mcp-server"],
      "env": {
        "SPIKE_LAND_API_KEY": "${SPIKE_LAND_API_KEY}"
      }
    },
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@anthropic/mcp-server-playwright"],
      "env": {}
    }
  }
}
